        title
            block title
                | &nbsp;- Markt Labs
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        block stylesheets
            link(rel='stylesheet', href='css/bootstrap.min.css')
            link(rel='stylesheet', type='text/css', href='css/main_style.css')
